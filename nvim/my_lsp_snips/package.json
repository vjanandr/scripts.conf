{
  "name": "my_lsp_snips",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "plaintext",
          "markdown",
          "tex",
          "html",
          "global"
        ],
        "path": "./snippets/plaintext.json"
      },
      {
        "language": "c",
        "path": "./snippets/c.json"
      },
      {
        "language": "cpp",
        "path": "./snippets/cpp.json"
      },
      {
        "language": "python",
        "path": "./snippets/python.json"
      },
      {
        "language": "fortran",
        "path": "./snippets/fortran.json"
      }
    ]
  }
}
